<p id="notice"><%= notice %></p>

<h1>Natural People</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Rut</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @natural_people.each do |natural_person| %>
      <tr>
        <td><%= natural_person.nombre %></td>
        <td><%= natural_person.apellido %></td>
        <td><%= natural_person.rut %></td>
        <td><%= link_to 'Show', natural_person %></td>
        <td><%= link_to 'Edit', edit_natural_person_path(natural_person) %></td>
        <td><%= link_to 'Destroy', natural_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Natural Person', new_natural_person_path %>
